      -
        code : |1-
         bResult		is boolean
         sResult		is string
         bForce64Bit	is boolean
         
         // Should we run the 32 or 64-bit PowerShell
         bForce64Bit = (SEL_3264 = 2)
         
         // Run the PowerShell script
         (bResult, sResult) = PSRunScript(EDT_SCRIPT, bForce64Bit)
         
         // Show the result in the window
         if bResult then
         	STC_RESULT			= "OK"
         	STC_RESULT.Color	= DarkGreen
         else
         	STC_RESULT.Color	= DarkRed
         	STC_RESULT			= "In Error"
         end
         
         // Show the report
         EDT_Execution_report = sResult
