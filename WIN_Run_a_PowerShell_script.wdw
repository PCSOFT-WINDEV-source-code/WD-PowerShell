     code : |1+
      procedure MyWindow()
      
      if PowerShellAvailable() = False then
      	Error("PowerShell is not installed on this computer. You cannot use this example.")
      	EndProgram()
      end
      
      
      
      
      if In64bitMode() then
      	// 64-bit mode is being used. We can only run 64-bit
      	SEL_3264		= 2
      	SEL_3264.State	= Grayed
      else
      	// 32-bit mode is being used. We can run both versions
      	// caution, in 32-bit
      	
      	if SysWindowsVersion(sysProcessor) = "32" then
      		// 32-bit Windows
      		SEL_3264		= 1
      		SEL_3264.State	= Grayed
      	else
      		// we are in 64-bit Windows, in a 32-bit project configuration.
      		// we can run both (PowerShell 32 or PowerShell 64)
      		// Switch to 64-bit by default. There are more modules available in 64-bit than in 32-bit
      		SEL_3264 = 2
      	end
      	
      end
      
      EDT_SCRIPT = [
      
      # Lists the shares defined on the current computer
      Get-WmiObject Win32_Share | Format-List
      
      ]
